<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>

<HBox xmlns:fx="http://javafx.com/fxml/1"
      prefHeight="300.0" prefWidth="400.0"        
      xmlns="http://javafx.com/javafx/8.0.40"  
      HBox.hgrow="NEVER"
      fx:controller="gui.controllers.QualityReportController">
       
    <ScrollPane hbarPolicy="NEVER" vbarPolicy="ALWAYS" >
        
        <Label fx:id="feedbackArea" maxWidth="370.0"
               wrapText="true">            
        </Label>    
        <HBox.margin>
            <Insets bottom="5.0" right="5.0" left="5.0" top="5.0"/>   
        </HBox.margin>
        <padding>
            <Insets bottom="5.0" right="5.0" left="5.0" top="5.0"/>   
        </padding>
            
    </ScrollPane>
</HBox>